#include <iostream>
#include <algorithm>
using namespace std;

int main() {
    int a[12];

    for (int i = 0; i < 12; i++)
        cin >> a[i];

    sort(a, a + 12);

    cout << "Ascending: ";
    for (int i = 0; i < 12; i++)
        cout << a[i] << " ";
    cout << endl;

    sort(a, a + 12, greater<int>());

    cout << "Descending: ";
    for (int i = 0; i < 12; i++)
        cout << a[i] << " ";
    cout << endl;

    return 0;
}
//------------------------------------------
#include <iostream>
using namespace std;

int main() {
    int n;
    cin >> n;

    int a[100];

    for (int i = 0; i < n; i++)
        cin >> a[i];

    int max_i = 0;
    for (int i = 1; i < n; i++)
        if (a[i] > a[max_i])
            max_i = i;

    for (int i = 0; i < max_i; i++)
        if (a[i] < 0)
            a[i] = 0;

    for (int i = 0; i < n; i++)
        cout << a[i] << " ";

    return 0;
}
