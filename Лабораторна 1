#include <iostream>
#include <cmath>
using namespace std;

int main() {
    double a, b, y;

    cin >> a;
    cin >> b;

    double x1 = cbrt(log(b / a));
    double x2 = cbrt(1 + (0.711 * log(b)) / x1);
    double x3 = cbrt(1 + (0.315 * log(a)) / x2);

    y = x3;

    cout << y;

    return 0;
}
