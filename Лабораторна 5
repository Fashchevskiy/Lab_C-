#include <iostream>
#include <cmath>
using namespace std;

int main() {
    double x, y;
    cout << "Enter 5 x values:\n";
    for (int i = 1; i <= 5; i++) {
        cout << "x" << i << " = ";
        cin >> x;
        y = pow(sin(x), 3) + fabs(5 * x - 1.5);
        cout << "y = " << y << endl;
    }
    return 0;
}
// -----------------------------------------------------------------------
#include <iostream>
#include <cmath>
using namespace std;

int main() {
    double sum = 0;
  

    for (int i = 1; i <= 20; i++) {
        sum += sqrt(i);
        
    }

    double average = sum / 20;
    cout << "The arithmetic mean of the roots of numbers from 1 to 20 = " << average << endl;

    return 0;
}
// -----------------------------------------------------------------------
#include <iostream>
#include <cmath>
#include <iomanip>
using namespace std;

int main() {
    double a, b, h, x, y;

    cout << "Enter a, b, h: ";
    cin >> a >> b >> h;

    cout << setw(10) << "X" << setw(20) << "Y" << endl;
    cout << "------------------------------------" << endl;

    for (x = a; x <= b; x += h) {
        double inside = 9 - x * x;
        if (inside < 0) {
            cout << setw(10) << x << setw(20) << "Inadmissible (ODZ)" << endl;
        } else {
            y = pow(x, 5) - sqrt(inside);
            cout << setw(10) << x << setw(20) << y << endl;
        }
    }

    return 0;
}
